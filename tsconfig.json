{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "pretty": true
  },
  "include": ["./*.ts", "./Abandonned/*.ts", "./Solved/*.ts"],
  "exclude": ["node_modules", "tools", "data", "dist"]
}
